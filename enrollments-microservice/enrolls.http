# Obtener todas las Matriculas Regitradas
GET http://localhost:5258/api/Enrollments/all

# Actualizar datos de Matricula
PUT http://localhost:5258/api/Enrollments/enroll/{enrollId}
http://localhost:5258/api/Enrollments/enroll/0
{
  "studentId": "1",
  "schoolId": "1",
  "fullName": "Shinji Ikari",
  "academicPerformance": 0,
  "credits": 28,
  "courses": [
    {
      "id": "1",
      "group": "A"
    },
    {
      "id": "3",
      "group": "C"
    }
  ],
  "schoolName": "Evangelion"
}

# Eliminar una Matricula Registrada
DELETE http://localhost:5258/api/Enrollments/enroll/{enrollId}
http://localhost:5258/api/Enrollments/enroll/2

# Obtener las matriculas de un Usuario
GET http://localhost:5258/api/Enrollments/user/{userId}
http://localhost:5258/api/Enrollments/user/1

# Obtener los cursos disponibles para matricular de un Usuario
GET http://localhost:5258/api/Enrollments/available/{userId}/{schoolId}
http://localhost:5258/api/Enrollments/available/1/1

# Obtener la Matricula por su id
GET http://localhost:5258/api/Enrollments/{enrollId}
http://localhost:5258/api/Enrollments/1

# Obtener las Matriculas de una Escuela por su id
GET http://localhost:5258/api/Enrollments/school/{schoolId}
http://localhost:5258/api/Enrollments/school/1
